
@{
    ViewBag.Title = "Crew";
}

<nav class="bar bar-standard">
    <a href="/homepage/homepage" id="home" class="btn btn-primary pull-left">
        Home
    </a>
    
    <h1 class="title">Friends</h1>
</nav>

<div class="content">
    

        <ul id="crew" class="table-view">

            <li id="crewClone" class="table-view-cell partCrew"><span class="crewName"></span>
            <button class="btn num" data-id="2">Request Pending</button></li>

        </ul>


    
</div>
@section scripts{

<script>

    var findMe = { page: { handlers: {} }, services: {} };
    console.warn("hello there");

    findMe.page.startUp = function () {

        console.warn("hello start");

        var myCrew = [{ name: 'Bozo', id: 99 }, { name: 'Sandy', id: 33 }, { name: 'Bob', id: 1 }, { name: 'Tupac', id: 10 }, { name: 'Jimi', id: 69 }];

        findMe.page.confirm(myCrew);

    }
    
    findMe.page.confirm = function(crew){

        console.warn("hello crew");

        console.warn(crew);

        var targetList = $("#crew");

        for (var i = 0; i < crew.length; i++) {

            var clone = $("#crewClone").clone();
            clone.removeAttr("id");

            $(".crewName", clone).text(crew[i].name);

            clone.appendTo(targetList);



        }

        

        



    }


    $(document).ready(findMe.page.startUp);




</script>

}
